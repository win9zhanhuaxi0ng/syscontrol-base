<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demofactory.syscontrol.dao.SysUserDao">
    <select id="findByAccount" parameterType="string" resultType="com.demofactory.syscontrol.domain.SysUser">
        select * from sys_user where account = #{account}
    </select>
</mapper>